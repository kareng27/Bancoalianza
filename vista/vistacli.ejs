        <div id="cajauno" class="cajas">

            <div class="titulocaja"><a href="/cuentacli">Cuentas</a></div>

        </div>

        
        

    </section>



    <from action="" name="frm" id="frm">
      <table class="table">
          <thead>
              <tr>
    
                  <th>Documento</th>
                  <th>NOMBRE</th>
                  <th>APELLIDO</th>
                  <th>CORREO</th>
                  <th>CELULAR</th>
                  <th>SEXO</th>
                  <th>DATE</th>
    
    
              </tr>
          </thead>
    
          <tbody>
           <% datos.forEach(function (datos){ %>    
              <tr>
                  <td><input type="text" value="<%=datos.CliDoc%>" name="documento" id="doc" class="doc"></td>
                  <td><input type="text" value="<%=datos.CliNom%>" name="nombre" id="nom" class="nom"></td>
                  <td><input type="text" value="<%=datos.CliApe%>" name="apellido" id="ape" class="ape"></td>
                  <td><input type="text" value="<%=datos.CliCorreo%>" name="correo" id="cor" class="cor"></td>
                  <td><input type="text" value="<%=datos.CliCelular%>" name="celular" id="cel" class="cel"></td>
                  <td><input type="text" value="<%=datos.CliSexo%>" name="sexo" id="sexo" class="sexo"></td>
                  <td><input type="text" value="<%=datos.CliFechaNac%>" name="fecha" id="fec" class="fec"></td>
              </tr>
           <%})%>
          </tbody>
        </table>
      </from>


                     

                   

<script src="https://unpkg.com/scrollreveal"></script>
